# -*- mode:shell-script -*-

export EDITOR=emacs
if [ -z "$GOPATH" ]; then 
    export GOPATH=~/go
    PATH=$PATH:$GOPATH/bin
fi

PATH=$HOME/bin:$PATH
PATH=$HOME/local/bin:$PATH

export HOMEBREW_NO_ANALYTICS=1

# Oh apple
export ARCHFLAGS=-Wno-error=unused-command-line-argument-hard-error-in-future
export PYTHONDONTWRITEBYTECODE=1
