# -*- mode:shell-script -*-

alias hbup='~/local/hbase/bin/start-hbase.sh && ~/local/hbase/bin/hbase thrift start'
alias hbdown='~/local/hbase/bin/stop-hbase.sh'
alias hbshell='~/local/hbase/bin/hbase shell'
alias zkup='sudo /usr/share/zookeeper/bin/zkServer.sh start'
alias zkdown='sudo /usr/share/zookeeper/bin/zkServer.sh stop'
